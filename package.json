{
  "name": "aheenam-vue-dashboard",
  "version": "0.1.6",
  "description": "A basic framework to create a dashboard",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "prepublish": "npm run test; npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aheenam/vue-dashboard.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "keywords": [
    "vuejs",
    "javascript",
    "dashboard",
    "aheenam"
  ],
  "author": "Rathes Sachchithananthan <rathes@aheenam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aheenam/vue-dashboard/issues"
  },
  "homepage": "https://github.com/Aheenam/vue-dashboard#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.2.0",
    "path": "^0.12.7",
    "vue": "^2.4.4",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0"
  },
  "peerDependencies": {
    "vue": "^2.4.4"
  }
}
